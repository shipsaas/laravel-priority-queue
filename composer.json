{
    "name": "shipsaas/laravel-priority-queue",
    "type": "library",
    "version": "1.1.0",
    "description": "Priority Queue implementation for your Laravel Applications",
    "keywords": [
        "laravel library",
        "laravel priority queue",
        "laravel queue",
        "laravel"
    ],
    "authors": [
        {
            "name": "Phat Tran (Seth Phat)",
            "email": "me@sethphat.com",
            "homepage": "https://github.com/sethsandaru",
            "role": "Sr.SWE"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^10|^11"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.20.0",
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^10",
        "orchestra/testbench": "^8|^9",
        "phpunit/php-code-coverage": "^9|^10",
        "friendsofphp/php-cs-fixer": "^3.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShipSaasPriorityQueue\\PriorityQueueServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ShipSaasPriorityQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShipSaasPriorityQueue\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": [
            "@php vendor/bin/phpunit --coverage-clover coverage.xml"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
